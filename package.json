{
  "name": "@expo/web-snack",
  "version": "0.0.0",
  "private": true,
  "description": "The Snack website",
  "main": "build/server",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn start:prod || yarn start:dev",
    "start:dev": "nodemon -e json,js,ts,tsx --watch src/server src/server/index.tsx --exec babel-node --extensions '.ts,.tsx'",
    "start:prod": "cross-env NODE_ENV=production nodemon build/server/index.js",
    "build": "cross-env NODE_ENV=production yarn build:server && yarn build:client",
    "build:server": "babel --source-maps  --extensions '.ts,.tsx' --out-dir build/ src/",
    "build:client": "cross-env NODE_ENV=production webpack",
    "clean": "yarn clean:server && yarn clean:client",
    "clean:server": "del build/",
    "clean:client": "del dist/",
    "lint": "tslint -t stylish -p . -c tslint.json",
    "typescript": "tsc",
    "test": "cross-env NODE_ENV=test jest --config jest/unit.config.js",
    "test:integration": "cross-env NODE_ENV=test jest --config jest/integration.config.js",
    "prestart:prod": "yarn build",
    "prebuild": "yarn clean"
  },
  "author": "Expo",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "universe/node",
      "universe/web"
    ]
